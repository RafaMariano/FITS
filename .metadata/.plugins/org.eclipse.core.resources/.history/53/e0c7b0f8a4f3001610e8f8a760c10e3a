package br.inpe.model;

import java.time.LocalDate;

public class Date {
		private static Date date;
		
		public synchronized static Date getInstance(){
			if(date == null)
				date = new Date();
			return date;
		}
		
		public String dateIsCorrect(int day, int month, int year){
			try{
				LocalDate.of(year, month, day);
			}
		}
}
