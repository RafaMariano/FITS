package inpe.api;

import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;
import java.io.InputStream;
import java.io.OutputStream;
import java.net.URL;
import java.util.List;
import java.util.Map;
import java.net.HttpURLConnection;

public class Main {

	public static void main(String[] args) throws IOException {
			
		for (int contt = 0; contt < 1000;contt++){
			for (int cont = 0; cont < 200; ++cont){
				URL urlObj = new URL("http://localhost:8080/API/rest/images/"+Integer.toString(cont));   
				
				HttpURLConnection  httpConnection = (HttpURLConnection) urlObj.openConnection();
				httpConnection.setRequestMethod("GET");
				Map<String, List<String>> responseMap = httpConnection.getHeaderFields();
				for (String chave : responseMap.keySet()) {
				    System.out.println(chave + " = ");

				    List<String> valores = responseMap.get(chave);
				    for (int i = 0; i < valores.size(); i++) {
				        String valor = valores.get(i);
				        System.out.println(valor + ", ");
				    }
				}
				
				}
		}
		}

	}


