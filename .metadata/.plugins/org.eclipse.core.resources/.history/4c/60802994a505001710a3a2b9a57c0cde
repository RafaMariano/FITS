package br.inpe.repository;

import java.util.List;

import org.springframework.data.mongodb.repository.MongoRepository;
import org.springframework.data.querydsl.QueryDslPredicateExecutor;

import br.inpe.filesystem.ImagesCollection;

public interface ImageRepository extends MongoRepository<ImagesCollection, Long>, QueryDslPredicateExecutor<ImagesCollection>{
	
	public List<ImagesCollection> findAll();
}
